{
	"name":        "pxn/phputils",
	"description": "Website Utilities Library",
	"version":     "0.1.6",
	"type":        "library",
	"homepage":    "http://poixson.com/",
	"license":     "GPL-3",
	"keywords": [
		"utilities"
	],
	"support": {
		"issues": "https://github.com/PoiXson/phpUtils/issues",
		"source": "https://github.com/PoiXson/phpUtils",
		"irc":    "irc://irc.esper.net/poixson"
	},
	"authors": [{
		"name":  "lorenzop",
		"role":  "Developer"
	}],
	"autoload": {
		"psr-4": {
			"pxn\\phpUtils\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"pxn\\phpUtils\\tests\\":    "tests/",
			"pxn\\phpUtils\\examples\\": "examples/"
		}
	},
	"archive": {
		"exclude": [
			"tests/",
			"examples/"
		]
	},
	"require": {
		"php":             "5.6.*",
		"twig/twig":       "1.23.*",
		"monolog/monolog": "^1.15.0"
	},
	"require-dev": {
		"symfony/console": "^2.7.2",
		"phpunit/phpunit": "5.0.*"
	}
}
