{
	"name":        "pxn/phputils",
	"description": "PHP Utils for PoiXson projects",
	"version":     "0.1.1",
	"type":        "library",
	"homepage":    "http://poixson.com/",
	"license":     "GPL-3",
	"support": {
		"issues": "https://github.com/PoiXson/phpUtils/issues",
		"source": "https://github.com/PoiXson/phpUtils",
		"irc":    "irc://irc.esper.net/poixson"
	},
	"authors": [{
		"name":  "lorenzop",
		"role":  "Developer"
	}],
	"autoload": {
		"psr-4": {
			"pxn\\phpUtils\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"pxn\\phpUtils\\tests\\":    "tests/",
			"pxn\\phpUtils\\Examples\\": "examples/"
		}
	},
	"archive": {
		"exclude": [
			"tests/",
			"example/"
		]
	},
	"require": {
		"php":             "5.6.*"
	},
	"require-dev": {
		"phpunit/phpunit": "4.7.*"
	}
}
